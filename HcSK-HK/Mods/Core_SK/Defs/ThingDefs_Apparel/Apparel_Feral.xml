			<bodyPartGroups>
				<li>Torso</li>
				<li>Arms</li>
				<li>Shoulders</li>
			</bodyPartGroups>
		</apparel>
		<thingCategories>
			<li>HeavyFullArmorCat</li>
		</thingCategories>
	</ThingDef>

