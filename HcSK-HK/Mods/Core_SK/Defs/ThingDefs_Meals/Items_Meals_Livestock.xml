<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

  <ThingDef Name="CatFood" ParentName="OrganicMeals">
    <defName>CatFood</defName>
    <label>Processed Cat Food</label>
    <description>Processed cat food, made bulkier by adding plant material and various artificial additives. Your cats are guaranteed to love it!</description>
    <ingestible>
      <foodType>AnimalProduct</foodType>
      <preferability>DesperateOnly</preferability>
      <nutrition>0.2</nutrition>
      <ingestEffect>EatMeat</ingestEffect>
      <ingestSound>RawMeat_Eat</ingestSound>
    </ingestible>
    <thingCategories>
		<li>Animalfood</li>
      <!--<li>CatFoods</li>-->
    </thingCategories>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
      </li>
    </comps>
    <graphicData>
      <texPath>Things/Item/CatFood</texPath>
    </graphicData>
    <statBases>
      <DeteriorationRate>.1</DeteriorationRate>
      <MarketValue>2</MarketValue>
    </statBases>
  </ThingDef>

</ThingDefs>