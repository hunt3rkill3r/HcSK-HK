   <RecipeDef ParentName="SurgeryFlesh">
      <defName>InstallPowerArm</defName>
      <label>Install Power Arm</label>
      <description>Installs a Power Arm.</description>
      <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
      <jobString>Installing Power Arm.</jobString>
      <workAmount>3200</workAmount>
      <ingredients>
         <li>
            <filter>
               <categories>
                  <li>Medicine</li>
               </categories>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>PowerArm</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <categories>
            <li>Medicine</li>
         </categories>
         <thingDefs>
            <li>PowerArm</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
         <li>LeftShoulder</li>
         <li>RightShoulder</li>
      </appliedOnFixedBodyParts>
      <addsHediff>PowerArm</addsHediff>
      <skillRequirements>
         <li>
            <skill>Medicine</skill>
            <minLevel>11</minLevel>
         </li>
      </skillRequirements>
    <researchPrerequisite>SK_BionicsII</researchPrerequisite>
   </RecipeDef>
   
   
