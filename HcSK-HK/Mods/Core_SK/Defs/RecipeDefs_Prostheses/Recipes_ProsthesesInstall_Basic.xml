  <RecipeDef ParentName="SurgeryFlesh">
		<defName>InstallSteelArm</defName>
		<label>Install Steel Arm</label>
		<description>Installs a Steel Arm.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing Steel Arm.</jobString>
      <workAmount>2800</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SteelArm</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>SteelArm</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>LeftShoulder</li>
			<li>RightShoulder</li>
		</appliedOnFixedBodyParts>
		<addsHediff>SteelArm</addsHediff>
      <skillRequirements>
         <li>
            <skill>Medicine</skill>
            <minLevel>2</minLevel>
         </li>
      </skillRequirements>
    <researchPrerequisite>SK_MedicineI</researchPrerequisite>
	</RecipeDef>
  
  
