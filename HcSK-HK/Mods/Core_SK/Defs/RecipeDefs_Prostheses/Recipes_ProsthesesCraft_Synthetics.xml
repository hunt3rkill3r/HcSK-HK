   <RecipeDef ParentName="RecipeSyntheticOrgan">
      <defName>CreateAIChip</defName>
      <label>Craft AI Chip</label>
      <description>Crafts an AI chip. It restores brain functions but is very hard to make. Produces 1.</description>
      <jobString>Crafting AI chip.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>CarbonAlloy</li>
               </thingDefs>
            </filter>
            <count>8</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>BioMicrochips</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>CarbonAlloy</li>
            <li>BioMicrochips</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <AIChip>1</AIChip>
      </products>
      <skillRequirements>
         <li>
            <skill>Crafting</skill>
            <minLevel>16</minLevel>
         </li>
      </skillRequirements>
    <researchPrerequisite>SK_BionicsII</researchPrerequisite>
   </RecipeDef>
   
   
