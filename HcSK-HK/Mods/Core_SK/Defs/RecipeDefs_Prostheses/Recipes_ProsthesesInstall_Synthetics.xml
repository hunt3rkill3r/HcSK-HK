   <RecipeDef ParentName="SurgeryFlesh">
      <defName>InstallAIChip</defName>
      <label>Install AI Chip</label>
      <description>Installs an AI Chip.</description>
      <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
      <jobString>Installing AI Chip.</jobString>
      <workAmount>4600</workAmount>
      <ingredients>
         <li>
            <filter>
               <categories>
                  <li>Medicine</li>
               </categories>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>AIChip</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <categories>
            <li>Medicine</li>
         </categories>
         <thingDefs>
            <li>AIChip</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
         <li>Brain</li>
      </appliedOnFixedBodyParts>
      <addsHediff>AIChip</addsHediff>
      <skillRequirements>
         <li>
            <skill>Medicine</skill>
            <minLevel>15</minLevel>
         </li>
      </skillRequirements>
    <researchPrerequisite>SK_BionicsII</researchPrerequisite>
   </RecipeDef>
   
   
