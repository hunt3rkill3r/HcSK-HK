<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

	<!--========================= Recipes (Body Parts) =============================-->
	
	
	<RecipeDef>
		<defName>MakeNaturalEye</defName>
		<label>Grow Natural Eye</label>
		<description>Grow a natural eye from BioMatter. Produces 1.</description>
		<jobString>Growing a natural eye.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Bodypart_growing</soundWorking>
		<workAmount>2500</workAmount>
		<skillRequirements>
			<li>
				<skill>Medicine</skill>
				<minLevel>12</minLevel>
			</li>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Biomatter</li>
					</thingDefs>
				</filter>
				<count>35</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Biomatter</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Eye>1</Eye>
		</products>
		<workSkill>Medicine</workSkill>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
	</RecipeDef>
	
	
