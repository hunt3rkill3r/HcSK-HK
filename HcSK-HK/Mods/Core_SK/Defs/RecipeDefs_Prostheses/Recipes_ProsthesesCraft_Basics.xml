   <RecipeDef ParentName="RecipeBasicProsthesis">
      <defName>CreateSteelArm</defName>
      <label>Craft Metallic Arm</label>
      <description>Crafts a metallic arm. Produces 1.</description>
      <jobString>Crafting metallic arm.</jobString>
      <soundWorking>EPOEAssembling</soundWorking>
      <products>
         <SteelArm>1</SteelArm>
      </products>
      <ingredients>
         <li>
            <filter>
					<categories>
						<li>Metallic</li>
                    </categories>
            </filter>
            <count>20</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Component</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Mechanism</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
                    </categories>
         <thingDefs>
            <li>Component</li>
            <li>Mechanism</li>
         </thingDefs>
      </fixedIngredientFilter>
      <skillRequirements>
         <li>
            <skill>Crafting</skill>
            <minLevel>6</minLevel>
         </li>
      </skillRequirements>
      <workSkill>Crafting</workSkill>
   </RecipeDef>
   
   
