<NameMakerDefs>


 <!-- ==================== Medieval Standard Faction ================= -->
  <RulePackDef>
    <defName>MedievalTimes_Faction_NameSet</defName>
    <rulePack>
      <rulesStrings>
	<li>name->House [outlander]</li>
	<li>name->Clan [outlander]</li>
	<li>name->[outlander]'s Kingdom</li>
	<li>name->[outlander] Estates</li>
	<li>name->[outlander]'s Royal Court</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>outlander</keyword>
          <pathList>
            <li>Words/Nouns/People_Outlanders</li>
            <li>Words/Nouns/People_Family</li>
          </pathList>
        </li>
      </rulesRaw>
    </rulePack>
	
	
 <!-- ==================== Medieval Raider Faction ================= -->	
  </RulePackDef>
    <RulePackDef>
    <defName>MedievalTimes_Raider_Faction_NameSet</defName>
    <rulePack>
      <rulesStrings>
			<li>name->[badassperson]'s [groupname]</li>
			<li>name->The [badasscolor][terrainfeature]  [groupname]</li>			
			<li>name->[badassperson] [terrainfeature]</li>	
	
      </rulesStrings>
      <rulesRaw>
	  
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
		<li Class="Rule_File">
          <keyword>badassperson_plural</keyword>
          <path>Words/Nouns/People_Badass_plural</path>
        </li>
		<li Class="Rule_File">
          <keyword>badassperson_plural_RP</keyword>
          <path>Words/Nouns/People_Badass_plural_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>groupname</keyword>
          <path>Words/Nouns/GroupNames</path>
        </li>		
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>Words/Nouns/Colors_Badass</path>
        </li>
		<li Class="Rule_File">
          <keyword>badasscolor_RP</keyword>
          <path>Words/Nouns/Colors_Badass_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
		<li Class="Rule_File">
          <keyword>terrainfeature_color</keyword>
          <path>Words/Nouns/TerrainFeatures_color</path>
        </li>
		<li Class="Rule_File">
          <keyword>terrainfeature_peredza</keyword>
          <path>Words/Nouns/TerrainFeatures_PEREDZA</path>
        </li>	
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  
</NameMakerDefs>
