<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

  <RecipeDef>
    <defName>OilRefining</defName>
    <label>Refine Crude Oil</label>
    <description>Refine crude oil into useable materials. Produces 35 polymers, 23 fuel, 10 sand, 5 sulphates, 2 paraffins and 3 synthetic ammonia.</description>
    <jobString>Refining crude oil.</jobString>
    <workAmount>1200</workAmount>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
	<ingredients>
     <li>
        <filter>
          <thingDefs>
	<li>CrudeOil</li>
          </thingDefs>
        </filter>
        <count>35</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
          <thingDefs>
	<li>CrudeOil</li>
          </thingDefs>
    </fixedIngredientFilter>
	    <products>
      <Polymers>35</Polymers>
      <Fuel>23</Fuel>
      <Sand>10</Sand>
      <Sulphates>5</Sulphates>
      <Paraffins>2</Paraffins>
      <SyntheticAmmonia>3</SyntheticAmmonia>
    </products>
 		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>6</minLevel>
			</li>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>


