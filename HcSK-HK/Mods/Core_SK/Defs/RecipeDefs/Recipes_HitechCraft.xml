<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

  <RecipeDef>
    <defName>BuildMagneticMaterial</defName>
    <label>Synthesize Magnetic Material</label>
    <description>Synthesize strong magnetic material from magnetic metals. Produces 5.</description>
    <jobString>Synthesizing magnetic material.</jobString>
    <workAmount>2400</workAmount>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Matter</li>
          </thingDefs>
        </filter>
        <count>30</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Sulphates</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
	  <li>
        <filter>
          <categories>
            <li>USLDBar</li>
          </categories>
        </filter>
        <count>10</count>
      </li>
	  <li>
        <filter>
          <categories>
            <li>SLDBar</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <products>
      <MagneticMaterial>5</MagneticMaterial>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Matter</li>
        <li>Sulphates</li>
            <li>CupronickelAlloy</li>
            <li>NichromeAlloy</li>
            <li>StelliteAlloy</li>
            <li>PobediteAlloy</li>
			<li>Plasteel</li>
			<li>AlnicoAlloy</li>
      </thingDefs>
    </fixedIngredientFilter>
 		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>18</minLevel>
			</li>
		</skillRequirements>
    <workSkill>Crafting</workSkill>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>
  
  
