	<ThingDef ParentName="ResourceBase">
		<defName>Cloth</defName>
		<label>Cloth</label>
		<description>Cloth is woven from the raw organic fibers of a Cotton Bush. These fibers are called Raw Cotton.</description>
    <graphicData>
		<texPath>Things/Item/Resource/Cloth</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<Color>(162,157,152)</Color>
    </graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>1.5</MarketValue>
			<Flammability>4.0</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
		</statBases>
		<tickerType>Rare</tickerType>
		<stackLimit>300</stackLimit>
		<stuffProps>
			<categories>
				<li>Fabric</li>
			</categories>
			<color>(162,157,152)</color>
			<allowColorGenerators>true</allowColorGenerators>
			<statFactors>
				<MaxHitPoints>1.0</MaxHitPoints>
				<Beauty>1.1</Beauty>
