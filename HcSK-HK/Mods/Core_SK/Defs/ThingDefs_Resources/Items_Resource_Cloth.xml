				<Insulation_Heat>0.95</Insulation_Heat>
				<MoveSpeed>1</MoveSpeed>
				<MarketValue>0.8</MarketValue>
				<WorkToMake>0.9</WorkToMake>
				<WorkToBuild>0.9</WorkToBuild>
				<BedRestEffectiveness>1.1</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>0.9</ImmunityGainSpeedFactor>
			</statFactors>
		</stuffProps>
		<thingCategories>
			<li>BTextiles</li>
		</thingCategories>
		<comps>
			<li>
				<compClass>CompColorable</compClass>
			</li>
		</comps>
		<colorGeneratorInTraderStock Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>5</weight>
					<only>RGBA(0.33,0.33,0.33,1)</only>
				</li>
				<li>
					<weight>10</weight>
					<min>RGBA(0.3,0.3,0.3,1)</min>
					<max>RGBA(0.9,0.9,0.9,1)</max>
