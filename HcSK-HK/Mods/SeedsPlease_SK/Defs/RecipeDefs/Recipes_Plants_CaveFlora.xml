<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedShimmershroom</defName>
    <label>extract shimmershroom seeds</label>
    <description>Extract seeds from Shimmershrooms.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawShimmershroom</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawShimmershroom</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedShimmershroom>5</SeedShimmershroom>
    </products>
    <researchPrerequisite>SK_FungiII</researchPrerequisite>
  </RecipeDef>
  
