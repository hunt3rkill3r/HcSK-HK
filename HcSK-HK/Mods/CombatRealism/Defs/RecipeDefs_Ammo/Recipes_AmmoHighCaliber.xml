<RecipeDefs>

  <RecipeDef Name="AmmoRecipeHighCaliber" Abstract="true">
    <workSpeedStat>SmithingSpeed</workSpeedStat>
	<effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>5000</workAmount>
    <workSkill>Crafting</workSkill>
    <targetCountAdjustment>60</targetCountAdjustment>
	<recipeUsers>
	  <!-- Need an empty list here or the ammo injector will throw a null ref exception -->
	</recipeUsers>
  </RecipeDef>
  
  <!-- ==================== 9x39mm Soviet sniper standard ========================== -->

  <RecipeDef ParentName="AmmoRecipeHighCaliber">
    <defName>MakeAmmo_9x39Soviet_FMJ</defName>
    <label>make 9x39mm Soviet sniper standard cartridge x60</label>
    <description>Craft 60 9x39mm Soviet sniper standard cartridges.</description>
    <jobString>Making 9x39mm Soviet sniper standard cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
					<categories>
						<li>SLDBar</li>
                    </categories>
        </filter>
        <count>9</count>
      </li>
      <li>
        <filter>
					<categories>
						<li>USLDBar</li>
                    </categories>
        </filter>
        <count>6</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Powder</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
                    </categories>
      <thingDefs>
        <li>Powder</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
		<Ammo_9x39Soviet_FMJ>60</Ammo_9x39Soviet_FMJ>
    </products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>8</minLevel>
			</li>
		</skillRequirements>
    <workSkill>Crafting</workSkill>
    <workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>
  
