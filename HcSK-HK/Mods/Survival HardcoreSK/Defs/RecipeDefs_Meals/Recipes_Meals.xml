<RecipeDefs>

	<RecipeDef>
		<defName>CookMealSalad</defName>
		<label>make quick salad</label>
		<description>A salad is one of the ways a colonist might process crops into a meal if there is no meat available to make bigger meals. Produces 6.</description>
		<jobString>Making Simple Salad.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workAmount>200</workAmount>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>FruitFoodRaw</li>
					</categories>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>ExtraPlantFoodRaw</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<MealSalad>6</MealSalad>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>FruitFoodRaw</li>
				<li>ExtraPlantFoodRaw</li>
			</categories>
			<disallowedSpecialFilters>
				<li>AllowPlantFood</li>
			</disallowedSpecialFilters>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>FruitFoodRaw</li>
				<li>ExtraPlantFoodRaw</li>
			</categories>
			<exceptedCategories>
				<li>EggsFertilized</li>
			</exceptedCategories>
		</defaultIngredientFilter>
		<recipeUsers>
			<li>TableButcherNeolithic</li>
			<li>TableButcher</li>
		</recipeUsers>
		<workSkill>Cooking</workSkill>
	</RecipeDef>


