<Buildings>
  <ThingDef ParentName="BuildingBase">
    <DefName>MovingRailStraight</DefName>
    <label>Straight Conveyor</label>
	<category>Building</category>
	<designationCategory>Industrial</designationCategory>
	<graphicData>
      <texPath>Things/Building/MovingFloor/base64_frame0</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <damageData>
        <rect>(0,0.35,1,0.3)</rect>
      </damageData>
	  <drawSize>(1,1)</drawSize>
    </graphicData>
    <uiIconPath>Things/Building/MovingFloor/base64_frame0</uiIconPath>
	<placingDraggableDimensions>1</placingDraggableDimensions>
	<altitudeLayer>FloorEmplacement</altitudeLayer>
	<thingClass>MovingFloor.MovingRailBase</thingClass>
	<passability>Standable</passability>
    <Description>A straight industrial conveyor for your inudstrial production grid.</Description>
	<drawerType>MapMeshAndRealTime</drawerType>
	<selectable>true</selectable>
	<repairEffect>Repair</repairEffect>	
	<soundImpactDefault>BulletImpactMetal</soundImpactDefault>
	<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
	<tickerType>Normal</tickerType>
	<size>(1,1)</size>
	<stuffCategories>
		<li>Metallic</li>
	</stuffCategories>
	<costStuffCount>25</costStuffCount>
	<costList>
		<ReinforcedConcrete>4</ReinforcedConcrete>
		<Component>10</Component>
		<Mechanism>5</Mechanism>
		<Rubber>5</Rubber>
		<ElectronicComponents>1</ElectronicComponents>
	</costList>
	<statBases>
		<MaxHitPoints>80</MaxHitPoints>
		<WorkToBuild>80</WorkToBuild>
		<Flammability>0.4</Flammability>
		<Beauty>-1</Beauty>
	</statBases>
	<researchPrerequisites>
		<li>Conveyors</li>
	</researchPrerequisites>
    <comps>
		<li Class="CompProperties_Power">
			<compClass>CompPowerTrader</compClass>
			<startElectricalFires>false</startElectricalFires>
			<basePowerConsumption>5</basePowerConsumption>
			<transmitsPower>true</transmitsPower>
		</li>
		<li Class="CompProperties_Glower">
			<glowRadius>2</glowRadius>
			<glowColor>(0,217,217,0)</glowColor>
		</li>
		<li Class="CompProperties_Flickable"/>
    </comps>
	<building>
		<soundAmbient>GeothermalPlant_Ambience</soundAmbient>
	</building>
	<castEdgeShadows>true</castEdgeShadows>
  </ThingDef>
  
